<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
             
     <ui:style>
        .labelMargin {
            position: relative;
            line-height: 25px;
            margin-left: 18px;

        }
        .spaceRight {
            margin-right: 10px;
        }
        
       
    </ui:style>

    <g:DockLayoutPanel unit="PX" debugId="dssProject">
       <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Service Name:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
                </g:west>
                <g:center>
                    <g:SimplePanel addStyleNames="{style.spaceRight}">
                        <g:SimplePanel>
                            <g:TextBox width="400px" height="22px" ui:field="fileName" debugId="dssProject"/>
                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Service Namespace:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
                </g:west>
                <g:center>
                    <g:SimplePanel addStyleNames="{style.spaceRight}">
                        <g:SimplePanel>
                            <g:TextBox width="400px" height="22px" ui:field="serviceNamespace" debugId="dssProject"/>
                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
                <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Service Group:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
                </g:west>
                <g:center>
                    <g:SimplePanel addStyleNames="{style.spaceRight}">
                        <g:SimplePanel>
                            <g:TextBox width="400px" height="22px" ui:field="serviceGroup" debugId="dssProject"/>
                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
         <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Description:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
                </g:west>
                <g:center>
                    <g:SimplePanel addStyleNames="{style.spaceRight}">
                        <g:SimplePanel>
                            <g:TextBox width="400px" height="22px" ui:field="description" debugId="dssProject"/>
                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
          <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Data Source ID:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
