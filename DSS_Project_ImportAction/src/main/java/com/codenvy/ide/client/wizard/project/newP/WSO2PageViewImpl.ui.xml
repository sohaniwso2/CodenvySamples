                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
               <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
                    <g:FlowPanel>
                        <g:Label text="Database Engine:" addStyleNames="{style.labelMargin}"/>
                    </g:FlowPanel>
                </g:west>
                <g:center>
                    <g:SimplePanel addStyleNames="{style.spaceRight}">
                        <g:SimplePanel>
                            <g:TextBox width="400px" height="22px" ui:field="engine" debugId="dssProject"/>
                        </g:SimplePanel>
                    </g:SimplePanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="50">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="140">
